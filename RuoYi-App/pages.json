{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [{
			"path": "pages/index",
			"style": {
				"navigationBarTextStyle": "black",
				"navigationStyle": "custom"
			}
		}, {
			"path": "pages/login",
			"style": {
				"navigationBarTitleText": "登录"
			}
		}, {
			"path": "pages/mine/index",
			"style": {
				"navigationStyle": "custom",
				"navigationBarTextStyle": "white",
				"enablePullDownRefresh": true
			}
		}, {
			"path": "pages/researchAndDevelopment",
			"style": {
				"navigationBarTextStyle": "black",
				"navigationStyle": "custom"
			}
		},{
			"path": "pages/common/webview/index",
			"style": {
				"navigationBarTitleText": "浏览网页"
			}
		}, {
			"path": "pages/common/textview/index",
			"style": {
				"navigationBarTitleText": "浏览文本"
			}
		}
	],
	"subPackages": [{
				"root": "pages/componentsA",
				"pages": [
					{
						"path" : "relation/relation",
						"style" : 
						{
							"navigationBarTitleText" : "联系我们",
							"enablePullDownRefresh" : false
						}
					}
				]
			}, {
				"root": "pages/componentsB",
				"pages": [{
						"path": "info/index",
						"style": {
							"navigationBarTitleText": "个人信息"
						}
					}, {
						"path": "info/edit",
						"style": {
							"navigationBarTitleText": "编辑资料"
						}
					}, {
						"path": "setting/index",
						"style": {
							"navigationBarTitleText": "应用设置"
						}
					}, {
						"path": "help/index",
						"style": {
							"navigationBarTitleText": "常见问题"
						}
					}, {
						"path": "about/index",
						"style": {
							"navigationBarTitleText": "关于我们"
						}
					}, 
					{
						"path": "common/notice/announcement",
						"style": {
							"navigationBarTitleText": "最新公告",
							"enablePullDownRefresh": true
						}
					},
					{
						"path": "common/notice/notificationlist",
						"style": {
							"navigationBarTitleText": "公司通知",
							"enablePullDownRefresh": true
						}
					},
					{
						"path": "common/notice/notification",
						"style": {
							"navigationBarTitleText": "通知详情",
							"enablePullDownRefresh": true
						}
					}
				]}],
				"tabBar": {
					"color": "#000000",
					"selectedColor": "#000000",
					"borderStyle": "white",
					"backgroundColor": "#ffffff",
					"list": [{
							"pagePath": "pages/index",
							"iconPath": "pages/componentsA/static/images/tabbar/repair_normal.png",
							"selectedIconPath": "pages/componentsA/static/images/tabbar/repair_active.png",
							"text": "骑行"
						},
						{
							"pagePath": "pages/mine/index",
							"iconPath": "pages/componentsA/static/images/tabbar/mine_normal.png",
							"selectedIconPath": "pages/componentsA/static/images/tabbar/mine_active.png",
							"text": "我的"
						}
					]
				},
				"globalStyle": {
					"navigationBarTextStyle": "black",
					"navigationBarTitleText": "问卷调查",
					"navigationBarBackgroundColor": "#FFFFFF"
				}
			}